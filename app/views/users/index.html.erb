<table>
	<tr>
		<td>name</td>
		<td>email</td>
		<td>password</td>
	</tr>
	<% @users.each do |user| %>
	<tr>
		<td><%= user.name%></td>
		<td><%= user.email%></td>
		<td><% user.password%></td>
		<td><%= link_to 'Show', user %></td>
		<td><%= link_to 'Edit', edit_user_path(user.id) %></td>
		<td><%= link_to 'Delete', user,
	               confirm: 'Are you sure?',method: :delete %></td>
	</tr>
	<%end%>
</table>
